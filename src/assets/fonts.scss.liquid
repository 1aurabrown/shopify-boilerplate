@font-face {
  font-family: "MyFont";
  src: url({{ "FontFile.eot" | asset_url }});
  src: url({{ "FontFile.eot?#iefix" | asset_url }}) format("embedded-opentype"),
       url({{ "FontFile.woff2" | asset_url }}) format("woff2"),
       url({{ "FontFile.woff" | asset_url }}) format("woff");
  font-weight: normal;
  font-style: normal;
}